{% extends "::layout.html.twig" %}
{% block title %}Historique{% endblock %}
{% block titre %}Historique{% endblock %}
{% block contenu %}
<div class="text-center">
    {% for t in types %}
    <a href="{{url('historique_'~t.slug)}}">
    <table class="categorie">
        <tr>
            <td class="contImage">
                <img class="wwimg-thumbnail wwimg-responsive" width="auto" height="auto" src="t.slug" alt="{{t.slug}}">
            </td>
        </tr>
        <tr>
            <td>
                {{t.libelle|capitalize}}
            </td>
        </tr>
    </table></a>
    {% endfor %}
</div>


{% endblock %}

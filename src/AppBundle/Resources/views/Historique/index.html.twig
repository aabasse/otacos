{% extends "::layout.html.twig" %}
{% block title %}Historique{% endblock %}
{% block titre %}Historique{% endblock %}
{% set liensFilAriane = [ {'url': url('accueil'), 'libelle':'accueil'}, {'url': '', 'libelle':'historique'}] %}
{% block contenu %}
<div class="text-center">
    {% for t in types %}
    <a href="{{url('historique_'~t.slug)}}">
    <table class="categorie">
        <tr>
            <td class="contImage">
                <i class="fa {{t.icon}} fa-3x"></i>
            </td>
        </tr>
        <tr>
            <td>
                {{t.libelle|capitalize}}
            </td>
        </tr>
    </table></a>
    {% endfor %}
</div>
{% endblock %}
